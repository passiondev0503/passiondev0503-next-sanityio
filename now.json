{
    "version": 2,
    "name": "leerob",
    "cleanUrls": true,
    "trailingSlash": false,
    "env": {
        "FIREBASE_PROJECT_ID": "leerob-blog",
        "FIREBASE_PRIVATE_KEY": "@firebase-private-key",
        "FIREBASE_CLIENT_EMAIL": "@firebase-client-email",
        "MAILCHIMP_LIST_ID": "@mailchimp-list-id",
        "MAILCHIMP_API_KEY": "@mailchimp-api-key",
        "UNSPLASH_ACCESS_KEY": "@unsplash-access-key",
        "GOOGLE_ENCRYPTION_KEY": "@google-encryption-key"
    },
    "redirects": [
        {
            "source": "/LeeRobinson.pdf",
            "destination": "/robinson-resume-2019.pdf"
        }
    ],
    "headers": [
        {
            "source": "/api/unsplash",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=1200, stale-while-revalidate=600"
                }
            ]
        },
        {
            "source": "/api/youtube",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=120, stale-while-revalidate=60"
                }
            ]
        },
        {
            "source": "/api/page-views",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=120, stale-while-revalidate=60"
                }
            ]
        }
    ]
}
